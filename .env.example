# Spring Profile
SPRING_PROFILES_ACTIVE=local

# Server Configuration
SERVER_PORT=8080

# Database Configuration (Aiven MySQL)
DB_URL=jdbc:mysql://your-aiven-host:3306/defaultdb
DB_USERNAME=your-db-username
